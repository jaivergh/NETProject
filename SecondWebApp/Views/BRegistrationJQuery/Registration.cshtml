@model SecondWebApp.Models.RegistrationViewModel
@{
    ViewBag.Title = "Registration";
}

<div class="panel panel-body">



    <div class="panel panel-body">
        <div class="col-md-3">
            <div class="panel panel-default">

                @{
                    Html.RenderAction("SideMenu", "EDynamicMenu");
                }
            </div>
        </div>
        <div class="col-md-9">

            <div class="form-group">
                <div class="col-md-12">
                    <h4>Registrate aquí</h4>
                </div>
            </div>

            <form id="registrationForm">
                <div class="form-group">
                    <div class="col-md-12">
                        @Html.TextBoxFor(m => m.UserName, new { @class = "form-control", @placeholder = "Nombre" })
                    </div>
                </div>
                <div class="form-group">
                    <div class="col-md-12">
                        @Html.TextBoxFor(m => m.EmailId, new { @class = "form-control", @placeholder = "Correo eléctronico" })
                    </div>
                </div>
                <div class="form-group">
                    <div class="col-md-12">
                        @Html.PasswordFor(m => m.Password, new { @class = "form-control", @placeholder = "Contraseña" })
                    </div>
                </div>
                <div class="form-group">
                    <div class="col-md-12">
                        @Html.TextBoxFor(m => m.Address, new { @class = "form-control", @placeholder = "Dirección" })
                    </div>
                </div>
                <div class="form-group">
                    <div class="col-md-12">
                        <a href="#" class="btn btn-primary" onclick="RegisterUser()">Enviar</a>
                    </div>
                </div>

            </form>
        </div>
    </div>

</div>

<script>
    var RegisterUser = function () {

        var data = $("#registrationForm").serialize();
        debugger;
        $.ajax({
            type: "post",
            url: "/BRegistrationJQuery/RegisterUser",
            data: data,
            success: function (response) {
                alert("Exito");
            }

        })

    }
</script>