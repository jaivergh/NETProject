@model IEnumerable<SecondWebApp.Models.MenuItem>

<div class="panel panel-default">
    <div class="panel-heading">
        <h4>Menú</h4>
    </div>
    <div class="panel-body">
        <ul class="list-group">
            @if (Model != null)
            {
                foreach (var menu in Model)
                {

                    <li class="list-group-item"><a href="@menu.Link" class="btn btn-primary btn-block">@menu.LinkName</a></li>
                }
            }

            <li class="list-group-item"><a href="http://google.com" class="btn btn-warning btn-block">Google</a></li>
            <li class="list-group-item"><a href="http://skyscrapercity.com" class="btn btn-danger btn-block">SkyScraperCity</a></li>
        </ul>

    </div>
</div>


<div class="thumbnail">
    <div class="panel panel-primary">
        <div class="panel-heading">
            <h3 class="panel-title">Mensaje del Director</h3>
        </div>
    </div>
    <img src="~/Content/Images/frog-1371919.jpg" alt="..." style="margin-top:20px" />
    <div class="caption">
        <h3>Don Sapito</h3>
        <p>&para;El mejor en todo, no lo dudes confia en Don Sapito&para;</p>
        <p><a href="#" class="btn btn-primary" role="button">Ver más</a></p>
    </div>
</div>
