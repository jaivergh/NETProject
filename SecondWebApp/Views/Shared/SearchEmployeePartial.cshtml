@model IEnumerable<SecondWebApp.Models.EmployeeViewModel>

@if (Model != null)
{
    foreach (var employee in Model)
    {
        <tr>
            <td>@employee.Name</td>
            <td>@employee.DepartmentName</td>
            <td>@employee.Address</td>
            <td><a href=""><i class="glyphicon glyphicon-eye-open"></i>Ver</a></td>
        </tr>
    }
}